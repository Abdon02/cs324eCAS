class timer{
  float timeElapsed;
  float interval;
  boolean pause;
  boolean reset;

  // Default constructor
  timer(){
    this.timeElapsed = 0;
    this.interval = 200;
  }
  
  // Customizable constructor
  timer(float interval){
    this.timeElapsed = 0;
    this.interval = interval;
  }
  
  
  

}
